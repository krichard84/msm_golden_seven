<h1>Director Details</h1>

<img src="<%= @director.image_url %>">

<h6> <%= @director.image_url%> </h6>

<h2><%= @director.name %></h2>

<h6>Date of Birth: <%= @director.dob %> </h6>

<p>
  <%= @director.bio %>
</p>

<p>
  <%= time_ago_in_words(@director.created_at) %> ago
</p>

<a href="/directors" class="btn btn-default">
  Go back
</a>

<a href="/directors/<%= @director.id %>/edit" class="btn btn-default">
  Edit director
</a>

<a href="/delete_director/<%= @director.id %>" class="btn btn-default">
  Delete director
</a>